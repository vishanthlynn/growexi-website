  "version": 2,
  "installCommand": "npm ci && cd client && npm ci",
  "buildCommand": "npm run build",
  "outputDirectory": "client/dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/admin/inquiries", "destination": "/admin/inquiries" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
